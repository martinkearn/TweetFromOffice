<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="TaskPaneApp">
  <Id>6d4ca57f-2cdb-46f9-ad22-2af76416cc2f</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>[Provider name]</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Tweet From Office" />
  <Description DefaultValue="A simple Office add-in that enables the posting of Tweets from within Office "/>
  <AppDomains>
    <AppDomain>http://twitter.com</AppDomain>
    <AppDomain>http://api.twitter.com</AppDomain>
    <AppDomain>https://twitter.com</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook" />
    <Host Name="Presentation" />
    <Host Name="Project" />
    <Host Name="Document" />
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="http://tweetfromoffice.azurewebsites.net/" />
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
</OfficeApp>
